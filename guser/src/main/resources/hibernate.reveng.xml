<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="blog_user" match-name="user"/>
  <table-filter match-catalog="blog_user" match-name="role"/>
  <table-filter match-catalog="blog_user" match-name="userinrole"/>
  <table-filter match-catalog="blog_user" match-name="userdetail"/>
  <table-filter match-catalog="blog_user" match-name="userstate"/>
</hibernate-reverse-engineering>